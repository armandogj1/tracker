{
  "name": "tracker",
  "version": "1.0.0",
  "description": "Issue Tracker using typescript and react",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "cd src/server && npm run build",
    "build:client": "cd src/client && npm run build && mv build ../../dist",
    "build:all": "npm run build:server && npm run build:client",
    "start": "node ./src/server/index.js",
    "dev:server": "cd src/server && npm run dev",
    "dev:client": "cd src/client && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/armandogj1/tracker.git"
  },
